{
  "name": "soui",
  "private": true,
  "version": "0.0.1",
  "description": "A react library developed with dumi",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "dev:shineout": "cross-env NODE_ENV=development CASE_ENV=test node ./webpack/config.dev.ts",
    "build:md": "cross-env NODE_ENV=development CASE_ENV=test node ./scripts/dev-doc.ts",
    "build": "pnpm --filter=./packages/* run build",
    "build:watch": "pnpm --filter=./packages/* run build:watch",
    "docs:build": "dumi build",
    "prepare": "husky install && dumi setup",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"packages/**/*.{css,less}\"",
    "lint:es": "eslint \"packages/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "father doctor && npm run build",
    "test": "jest"
  },
  "authors": ["chenlihao@shein.com"],
  "license": "MIT",
  "files": ["dist"],
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "lint-staged": {
    "*.{md,json}": ["prettier --write --no-error-on-unmatched-pattern"],
    "*.{css,less}": ["stylelint --fix", "prettier --write"],
    "*.{js,jsx}": ["eslint --fix", "prettier --write"],
    "*.{ts,tsx}": ["eslint --fix", "prettier --parser=typescript --write"]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jest/types": "^29.5.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.1",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@umijs/lint": "^4.0.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.4",
    "dumi": "^2.1.21",
    "dumi-theme-antd-style": "0.22.0",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "html-loader": "^4.2.0",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.0.3",
    "markdown-it": "^13.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "raw-loader": "^4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.7",
    "react-test-renderer": "^18.2.0",
    "soui": "link:",
    "style-loader": "^3.3.3",
    "stylelint": "^14.9.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.0",
    "unified": "^10.1.2",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "react-jss": "10.9.2"
  }
}
