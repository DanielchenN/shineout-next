{
  "compilerOptions": {
    "strict": true,
    "declaration": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "module": "ES2020",
    "target": "ES5",
    "lib": ["ES2020", "DOM"],
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"]
    }
  },
  "includes": [".dumi/**/*", ".dumirc.ts", "packages/**/*"]
}
